msgid "Privileges"
msgstr "Rechte"

#: ../usr/lib/usermanager/usermanager.py:106
msgid "Password"
msgstr "Passwort"

#: ../usr/lib/usermanager/usermanager.py:108
msgid "Last changed"
msgstr "Zuletzt geändert"

#: ../usr/lib/usermanager/usermanager.py:109
msgid "Valid until"
msgstr "Gültig bis"

#: ../usr/lib/usermanager/usermanager.py:110
msgid "Always"
msgstr "Immer"

#: ../usr/lib/usermanager/usermanager.py:111
