msgstr "Warnen, bevor das Passwort ausläuft, nach"

#: ../usr/lib/usermanager/usermanager.py:114
msgid "Disable account when password expires after"
msgstr "Konto deaktivieren, wenn das Passwort ausläft, nach"

#: ../usr/lib/usermanager/usermanager.py:115
msgid "Enforce minimum password age"
msgstr "Erzwinge minimale Passwortgültigkeit "

#: ../usr/lib/usermanager/usermanager.py:116
msgid "days"
msgstr "Tage"

#: ../usr/lib/usermanager/usermanager.py:121
msgid "Group settings"
msgstr "Gruppeneinstellungen"

#: ../usr/lib/usermanager/usermanager.py:122
msgid "Group name"
msgstr "Gruppenname"

#: ../usr/lib/usermanager/usermanager.py:123
msgid "Group ID"
msgstr "Gruppen-ID"

#: ../usr/lib/usermanager/usermanager.py:124
msgid "Available accounts"
msgstr "Verfügbare Konten"

#: ../usr/lib/usermanager/usermanager.py:125
msgid "Selected accounts"
msgstr "Gewählte Konten"

#: ../usr/lib/usermanager/usermanager.py:267
msgid "Remove user"
msgstr "Benutzer entfernen"

#: ../usr/lib/usermanager/usermanager.py:269
#, python-format
msgid ""
"Are you sure you want to remove the following user:\n"
"\n"
"'%(user)s'"
msgstr ""
"Wirklich den folgenden Benutzer entfernen:\n"
"\n"
"'%(user)s'"

#: ../usr/lib/usermanager/usermanager.py:275
#, python-format
msgid "User successfully removed: %(user)s"
msgstr "Folgender Benutzer erfolgreich entfernt: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:279
#, python-format
msgid "Could not remove user: %(user)s %(retmsg)s"
msgstr "Konnte den folgenden Benutzer nicht entfernen: %(user)s %(retmsg)s "

#: ../usr/lib/usermanager/usermanager.py:281
msgid "You cannot remove the currently logged in user"
msgstr "Der zur Zeit angemeldete Benutzer kann nicht entfernt werden"

#: ../usr/lib/usermanager/usermanager.py:284
msgid "Select user directory"
msgstr "Benutzerverzeichnis wählen"

#: ../usr/lib/usermanager/usermanager.py:366
msgid "You need to provide a primary group for a new user"
msgstr "Für jeden Benutzer muß eine primäre Gruppe angegeben werden"

#: ../usr/lib/usermanager/usermanager.py:395
msgid "You need to provide a password for a new user"
msgstr "Für den neuen Benutzer muß ein Passwort angegeben werden"

#: ../usr/lib/usermanager/usermanager.py:398
msgid "You need to provide a name for a new user"
msgstr "Für den neuen Benutzer muß ein Name angegeben werden"

#: ../usr/lib/usermanager/usermanager.py:410
msgid "Save user settings"
msgstr "Benutzereinstellungen speichern"

#: ../usr/lib/usermanager/usermanager.py:421
#, python-format
msgid "User saved: %(user)s"
msgstr "Benutzer gespeichert: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:431
#, python-format
msgid "Nothing was changed on user: %(user)s"
msgstr "Der Benutzer wurde nicht geändert: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:486
msgid "Select user image"
msgstr "Ein Bild des Benutzers wählen"

#: ../usr/lib/usermanager/usermanager.py:556
msgid "Remove group"
msgstr "Gruppe entfernen"

#: ../usr/lib/usermanager/usermanager.py:558
#, python-format
msgid ""
"Are you sure you want to remove the following group:\n"
"\n"
"'%(group)s'"
msgstr ""
"Wirklich die folgende Gruppe entfernen:\n"
"\n"
"'%(group)s'"

#: ../usr/lib/usermanager/usermanager.py:563
#, python-format
msgid "Group successfully removed: %(group)s"
msgstr "Folgende Gruppe erfolgreich entfernt: %(group)s"

#: ../usr/lib/usermanager/usermanager.py:567
#, python-format
msgid "Could not remove group: %(group)s %(retmsg)s"
msgstr "Konnte die folgende Gruppe nicht entfernen: %(group)s %(retmsg)s"

#: ../usr/lib/usermanager/usermanager.py:569
msgid "You cannot remove the currently logged in user's primary group"
msgstr ""
"Die primäre Gruppe des zur Zeit angemeldeten Benutzers kann nicht entfernt "
"werden"

#: ../usr/lib/usermanager/usermanager.py:587
msgid "Remove account"
msgstr "Konto entfernen"

#: ../usr/lib/usermanager/usermanager.py:587
msgid "You cannot remove an account from its own primary group"
msgstr "Ein Konto kann nicht aus der eigenen primären Gruppe entfernt werden"

#: ../usr/lib/usermanager/user.py:254
#, python-format
msgid "Cannot remove group '%(group)s': not empty."
msgstr "Konnte die folgende Gruppe nicht entfernen: '%(group)s' nicht leer."

#: ../usr/lib/usermanager/user.py:256 ../usr/lib/usermanager/user.py:328
#, python-format
msgid "The command '%(cmd)s' returned error code: %(ret)d."
msgstr "Der Befehl '%(cmd)s' führte zu dem Fehlercode: %(ret)d."

#: ../usr/lib/usermanager/user.py:326
#, python-format
msgid ""
"Cannot remove user '%(user)s': processes started by user are still running."
msgstr ""
"Konnte den Benutzer '%(user)s'  nicht entfernen: Es laufen von noch dem "
"Benutzer gestartete Prozesse."

#: ../usr/share/kde4/services/kcm_usermanager.desktop.in.h:1
#: ../usr/share/applications/usermanager.desktop.in.h:2
msgid "User management"
msgstr "Benutzerverwaltung"

#: ../usr/share/kde4/services/kcm_usermanager.desktop.in.h:2
#: ../usr/share/applications/usermanager.desktop.in.h:1
msgid "Manage users"
msgstr "Benutzer verwalten"
