msgstr "Användarinställningar"

#: ../usr/lib/usermanager/usermanager.py:96
msgid "Details"
msgstr "Detaljer"

#: ../usr/lib/usermanager/usermanager.py:97
msgid "Status"
msgstr "Status"

#: ../usr/lib/usermanager/usermanager.py:98
msgid "Login name"
msgstr "Inloggningsnamn: "

#: ../usr/lib/usermanager/usermanager.py:99
msgid "Real name"
msgstr "Fullständigt namn"

