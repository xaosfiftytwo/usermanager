#: ../usr/lib/usermanager/usermanager.py:270
msgid "Remove user"
msgstr "Ta bort användare"

#: ../usr/lib/usermanager/usermanager.py:272
#, python-format
msgid ""
"Are you sure you want to remove the following user:\n"
"\n"
"'%(user)s'"
msgstr ""
"Är du säkert på att du vill ta bort följande användare?\n"
"\n"
"'%(user)s'"

#: ../usr/lib/usermanager/usermanager.py:278
#, python-format
msgid "User successfully removed: %(user)s"
msgstr "Användaren togs bort utan problem: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:282
#, python-format
msgid "Could not remove user: %(user)s %(retmsg)s"
msgstr "Användaren kunde inte tas bort: %(user)s %(retmsg)s"

#: ../usr/lib/usermanager/usermanager.py:284
msgid "You cannot remove the currently logged in user"
msgstr "Du kan inte ta bort en användare som är inloggad"

#: ../usr/lib/usermanager/usermanager.py:287
msgid "Select user directory"
msgstr ""

#: ../usr/lib/usermanager/usermanager.py:369
msgid "You need to provide a primary group for a new user"
msgstr "Ange en primärgrupp för den nya användaren"

#: ../usr/lib/usermanager/usermanager.py:398
msgid "You need to provide a password for a new user"
msgstr "Du behöver ange ett lösenord för den nya användaren"

#: ../usr/lib/usermanager/usermanager.py:401
msgid "You need to provide a name for a new user"
msgstr "Ange ett namn för den nya användaren"

#: ../usr/lib/usermanager/usermanager.py:413
msgid "Save user settings"
msgstr "Spara användarinställningar"

#: ../usr/lib/usermanager/usermanager.py:424
#, python-format
msgid "User saved: %(user)s"
msgstr "Användare sparad: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:434
#, python-format
msgid "Nothing was changed on user: %(user)s"
msgstr "Ingenting ändrades för användare: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:489
msgid "Select user image"
msgstr "Välj bild"

#: ../usr/lib/usermanager/usermanager.py:559
msgid "Remove group"
msgstr "Ta bort grupp"

#: ../usr/lib/usermanager/usermanager.py:561
#, python-format
msgid ""
"Are you sure you want to remove the following group:\n"
"\n"
"'%(group)s'"
msgstr ""
"Är du säkert på att du vill ta bort följande grupp?\n"
"\n"
"'%(group)s'"

#: ../usr/lib/usermanager/usermanager.py:566
#, python-format
msgid "Group successfully removed: %(group)s"
msgstr "Gruppen togs bort utan problem: %(group)s"

#: ../usr/lib/usermanager/usermanager.py:570
#, python-format
msgid "Could not remove group: %(group)s %(retmsg)s"
msgstr "Kunde inte ta bort gruppen: %(group)s %(retmsg)s"

#: ../usr/lib/usermanager/usermanager.py:572
msgid "You cannot remove the currently logged in user's primary group"
msgstr "Du kan inte ta bort gruppen som en inloggad användare tillhör"

#: ../usr/lib/usermanager/usermanager.py:590
msgid "Remove account"
msgstr "Tag bort konto"

#: ../usr/lib/usermanager/usermanager.py:590
msgid "You cannot remove an account from its own primary group"
msgstr "Du kan inte ta bort ett konto från sin egen primärgrupp"

#: ../usr/lib/usermanager/user.py:250
#, python-format
msgid "Cannot remove group '%(group)s': not empty."
msgstr "Kan inte ta bort gruppen '%(group)s': gruppen är inte tom."

#: ../usr/lib/usermanager/user.py:252 ../usr/lib/usermanager/user.py:324
#, python-format
msgid "The command '%(cmd)s' returned error code: %(ret)d."
msgstr "Kommandot '%(cmd)s' gav följande felkod: %(ret)d."

#: ../usr/lib/usermanager/user.py:322
#, python-format
msgid ""
"Cannot remove user '%(user)s': processes started by user are still running."
msgstr ""
"Kan inte ta bort användare '%(user)s': processer startade av användaren körs "
"fortfarande."

