msgstr "Persoonlijke map"

#: ../usr/lib/usermanager/usermanager.py:105
msgid "Shell"
msgstr "Shell"

#: ../usr/lib/usermanager/usermanager.py:106
msgid "Enabled"
msgstr "Ingeschakeld"

#: ../usr/lib/usermanager/usermanager.py:107
msgid "Disabled"
msgstr "Uitgeschakeld"

#: ../usr/lib/usermanager/usermanager.py:110
msgid "Privileges"
msgstr "Rechten"

#: ../usr/lib/usermanager/usermanager.py:112
msgid "Password"
msgstr "Wachtwoord"

#: ../usr/lib/usermanager/usermanager.py:114
msgid "Last changed"
msgstr "Laatst gewijzigd"

#: ../usr/lib/usermanager/usermanager.py:115
msgid "Valid until"
msgstr "Geldig tot"

#: ../usr/lib/usermanager/usermanager.py:116
msgid "Always"
msgstr "Altijd"

#: ../usr/lib/usermanager/usermanager.py:117
msgid "Password aging"
msgstr "Wachtwoord geldigheid"

#: ../usr/lib/usermanager/usermanager.py:118
msgid "Require password after"
msgstr "Wachtwoord vereisd na"

#: ../usr/lib/usermanager/usermanager.py:119
msgid "Warn before password expires after"
msgstr "Waarschuw voordat het wachtwoord verloopt na"

#: ../usr/lib/usermanager/usermanager.py:120
msgid "Disable account when password expires after"
msgstr "Schakel het account uit wanneer het wachtwoord verloopt na"

#: ../usr/lib/usermanager/usermanager.py:121
msgid "Enforce minimum password age"
msgstr "Dwing minimum geldigheid van het wachtwoord af"

#: ../usr/lib/usermanager/usermanager.py:122
msgid "days"
msgstr "dagen"

#: ../usr/lib/usermanager/usermanager.py:127
msgid "Group settings"
msgstr "Groepsinstellingen"

#: ../usr/lib/usermanager/usermanager.py:128
msgid "Group name"
msgstr "Groepsnaam"

#: ../usr/lib/usermanager/usermanager.py:129
msgid "Group ID"
msgstr "Groep ID"

#: ../usr/lib/usermanager/usermanager.py:130
msgid "Available accounts"
msgstr "Beschikbare accounts"

#: ../usr/lib/usermanager/usermanager.py:131
msgid "Selected accounts"
msgstr "Geselecteerde accounts"

#: ../usr/lib/usermanager/usermanager.py:273
msgid "Remove user"
msgstr "Gebruiker verwijderen"

#: ../usr/lib/usermanager/usermanager.py:275
#, python-format
msgid ""
"Are you sure you want to remove the following user:\n"
"\n"
"'%(user)s'"
msgstr ""
"Weet u zeker dat u de volgende gebruiker wilt verwijderen:\n"
"\n"
"'%(user)s'"

#: ../usr/lib/usermanager/usermanager.py:281
#, python-format
msgid "User successfully removed: %(user)s"
msgstr "Gebruiker succesvol verwijderd: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:285
#, python-format
msgid "Could not remove user: %(user)s %(retmsg)s"
msgstr "Kon gebruiker niet verwijderen: %(user)s %(retmsg)s"

#: ../usr/lib/usermanager/usermanager.py:287
msgid "You cannot remove the currently logged in user"
msgstr "De huidig ingelogde gebruiker kan niet worden verwijderd"

#: ../usr/lib/usermanager/usermanager.py:293
msgid "Select user directory"
msgstr "Selecteer gebruikersmap"

#: ../usr/lib/usermanager/usermanager.py:375
msgid "You need to provide a primary group for a new user"
msgstr "Geef een primaire groep aan voor de nieuwe gebruiker"

#: ../usr/lib/usermanager/usermanager.py:404
msgid "You need to provide a password for a new user"
msgstr "Geef een wachtwoord op voor de nieuwe gebruiker"

#: ../usr/lib/usermanager/usermanager.py:407
msgid "You need to provide a name for a new user"
msgstr "Geef een naam op voor de nieuwe gebruiker"

#: ../usr/lib/usermanager/usermanager.py:419
msgid "Save user settings"
msgstr "Bewaar gebruikersinstellingen"

#: ../usr/lib/usermanager/usermanager.py:430
#, python-format
msgid "User saved: %(user)s"
msgstr "Gebruiker bewaard: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:440
#, python-format
msgid "Nothing was changed on user: %(user)s"
msgstr "Niets gewijzigd aan gebruiker: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:495
msgid "Select user image"
msgstr "Selecteer gebruikersafbeelding"

#: ../usr/lib/usermanager/usermanager.py:565
msgid "Remove group"
msgstr "Groep verwijderen"

#: ../usr/lib/usermanager/usermanager.py:567
#, python-format
msgid ""
"Are you sure you want to remove the following group:\n"
"\n"
"'%(group)s'"
msgstr ""
"Weet u zeker dat u de volgende groep wilt verwijderen:\n"
"\n"
"'%(group)s'"

#: ../usr/lib/usermanager/usermanager.py:572
#, python-format
msgid "Group successfully removed: %(group)s"
msgstr "Groep succesvol verwijderd: %(group)s"

#: ../usr/lib/usermanager/usermanager.py:576
#, python-format
msgid "Could not remove group: %(group)s %(retmsg)s"
msgstr "Kon groep niet verwijderen: %(group)s %(retmsg)s"

#: ../usr/lib/usermanager/usermanager.py:578
msgid "You cannot remove the currently logged in user's primary group"
msgstr "De groep van de huidig ingelogde gebruiker kan niet worden verwijderd"

#: ../usr/lib/usermanager/usermanager.py:596
msgid "Remove account"
msgstr "Account verwijderen"

#: ../usr/lib/usermanager/usermanager.py:596
msgid "You cannot remove an account from its own primary group"
msgstr "Een account kan niet verwijderd worden van zijn eigen primaire groep"

#: ../usr/lib/usermanager/user.py:260
#, python-format
msgid "Cannot remove group '%(group)s': not empty."
msgstr "Kan groep '%(group)s' niet verwijderen: niet leeg."

#: ../usr/lib/usermanager/user.py:262 ../usr/lib/usermanager/user.py:334
#, python-format
msgid "The command '%(cmd)s' returned error code: %(ret)d."
msgstr "Het commando '%(cmd)s' gaf een error: %(ret)d."

#: ../usr/lib/usermanager/user.py:332
#, python-format
msgid ""
"Cannot remove user '%(user)s': processes started by user are still running."
msgstr ""
"Kan gebruiker '%(user)s' niet verwijderen: er lopen nog processen die door "
"de gebruiker gestart zijn."

#: ../usr/share/kde4/services/kcm_usermanager.desktop.in.h:1
#: ../usr/share/applications/usermanager.desktop.in.h:2
msgid "User management"
msgstr "Gebruikersbeheerder"

#: ../usr/share/kde4/services/kcm_usermanager.desktop.in.h:2
#: ../usr/share/applications/usermanager.desktop.in.h:1
msgid "Manage users"
msgstr "Beheer gebruikers"
