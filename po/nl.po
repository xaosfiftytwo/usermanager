#: ../usr/lib/usermanager/usermanager.py:95
msgid "Group accounts"
msgstr "Groep accounts"

#: ../usr/lib/usermanager/usermanager.py:97
msgid "User settings"
msgstr "Gebruikersinstellingen"

#: ../usr/lib/usermanager/usermanager.py:98
msgid "Details"
msgstr "Details"

#: ../usr/lib/usermanager/usermanager.py:99
msgid "Status"
msgstr "Status"

#: ../usr/lib/usermanager/usermanager.py:100
msgid "Login name"
msgstr "Loginnaam: "

