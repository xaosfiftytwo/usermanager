#: ../usr/lib/usermanager/usermanager.py:88
msgid "User manager"
msgstr "Gebruikersbeheerder"

#: ../usr/lib/usermanager/usermanager.py:89
msgid "Show system users"
msgstr "Toon systeemgebruikers"

#: ../usr/lib/usermanager/usermanager.py:90
msgid "Users"
msgstr "Gebruikers"

#: ../usr/lib/usermanager/usermanager.py:91
msgid "Groups"
msgstr "Groepen"

#: ../usr/lib/usermanager/usermanager.py:93
