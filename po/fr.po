msgid "Privileges"
msgstr "Privilèges"

#: ../usr/lib/usermanager/usermanager.py:110
msgid "Password"
msgstr "Mot de passe"

#: ../usr/lib/usermanager/usermanager.py:112
msgid "Last changed"
msgstr "Dernière modification"

#: ../usr/lib/usermanager/usermanager.py:113
msgid "Valid until"
msgstr "valide jusqu'au"

#: ../usr/lib/usermanager/usermanager.py:114
msgid "Always"
msgstr "Toujours"

#: ../usr/lib/usermanager/usermanager.py:115
msgid "Password aging"
msgstr "Expiration du mot de passe"

#: ../usr/lib/usermanager/usermanager.py:116
msgid "Require password after"
msgstr "Demande du mot de passe après"

#: ../usr/lib/usermanager/usermanager.py:117
msgid "Warn before password expires after"
msgstr "Avertir avant expiration du mot de passe après"

#: ../usr/lib/usermanager/usermanager.py:118
msgid "Disable account when password expires after"
msgstr "Desactiver le compte quand le mot de passe expire après"

#: ../usr/lib/usermanager/usermanager.py:119
msgid "Enforce minimum password age"
msgstr "Imposer un âge minimum au mot de passe"

#: ../usr/lib/usermanager/usermanager.py:120
msgid "days"
msgstr "jours"

#: ../usr/lib/usermanager/usermanager.py:125
msgid "Group settings"
msgstr "Paramètres du groupe"

#: ../usr/lib/usermanager/usermanager.py:126
msgid "Group name"
msgstr "Nom du groupe"

#: ../usr/lib/usermanager/usermanager.py:127
msgid "Group ID"
msgstr "Identifiant du groupe"

#: ../usr/lib/usermanager/usermanager.py:128
msgid "Available accounts"
msgstr "Comptes disponibles"

#: ../usr/lib/usermanager/usermanager.py:129
msgid "Selected accounts"
msgstr "Comptes sélectionnés"

#: ../usr/lib/usermanager/usermanager.py:271
msgid "Remove user"
msgstr "Supprimer l'utilisateur"

#: ../usr/lib/usermanager/usermanager.py:273
#, python-format
msgid ""
"Are you sure you want to remove the following user:\n"
"\n"
"'%(user)s'"
msgstr ""
"Êtes-vous certain de vouloir supprimer l'utilisateur:\n"
"\n"
"'%(user)s'"

#: ../usr/lib/usermanager/usermanager.py:279
#, python-format
msgid "User successfully removed: %(user)s"
msgstr "Utilisateur supprimé: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:283
#, python-format
msgid "Could not remove user: %(user)s %(retmsg)s"
msgstr "Impossible de supprimer l'utilisateur: %(user)s %(retmsg)s"

#: ../usr/lib/usermanager/usermanager.py:285
msgid "You cannot remove the currently logged in user"
msgstr "Vous ne pouvez pas supprimer l'utilisateur actuellement connecté"

#: ../usr/lib/usermanager/usermanager.py:288
msgid "Select user directory"
msgstr "Sélectionnez le répertoire utilisateur"

#: ../usr/lib/usermanager/usermanager.py:370
msgid "You need to provide a primary group for a new user"
msgstr "Vous devez définir un groupe principal pour tout nouvel utilisateur"

#: ../usr/lib/usermanager/usermanager.py:399
msgid "You need to provide a password for a new user"
msgstr "Vous devez définir un mot de passe pour tout nouvel utilisateur"

#: ../usr/lib/usermanager/usermanager.py:402
msgid "You need to provide a name for a new user"
msgstr "Vous devez définir le nom de tout nouvel utilisateur"

#: ../usr/lib/usermanager/usermanager.py:414
msgid "Save user settings"
msgstr "Enregistrer les paramètres utilisateur"

#: ../usr/lib/usermanager/usermanager.py:425
#, python-format
msgid "User saved: %(user)s"
msgstr "Utilisateur enregistré: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:435
#, python-format
msgid "Nothing was changed on user: %(user)s"
msgstr "Aucun changement pour l'utilisateur: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:490
msgid "Select user image"
msgstr "Choisir l'image de l'utilisateur"

#: ../usr/lib/usermanager/usermanager.py:560
msgid "Remove group"
msgstr "Supprimer le groupe"

#: ../usr/lib/usermanager/usermanager.py:562
#, python-format
msgid ""
"Are you sure you want to remove the following group:\n"
"\n"
"'%(group)s'"
msgstr ""
"Êtes-vous certain de vouloir supprimer le groupe suivant:\n"
"\n"
"'%(group)s'"

#: ../usr/lib/usermanager/usermanager.py:567
#, python-format
msgid "Group successfully removed: %(group)s"
msgstr "Groupe supprimé: %(group)s"

#: ../usr/lib/usermanager/usermanager.py:571
#, python-format
msgid "Could not remove group: %(group)s %(retmsg)s"
msgstr "Impossible de supprimer le groupe: %(group)s %(retmsg)s"

#: ../usr/lib/usermanager/usermanager.py:573
msgid "You cannot remove the currently logged in user's primary group"
msgstr ""
"Vous ne pouvez pas supprimer un groupe appartenant au groupe primaire de "
"l'utilisateur actuel"

#: ../usr/lib/usermanager/usermanager.py:591
msgid "Remove account"
msgstr "Suppression du compte"

#: ../usr/lib/usermanager/usermanager.py:591
msgid "You cannot remove an account from its own primary group"
msgstr "Vous ne pouvez pas supprimer un compte de son propre groupe principal"

#: ../usr/lib/usermanager/user.py:258
#, python-format
msgid "Cannot remove group '%(group)s': not empty."
msgstr "Impossible de supprimer le groupe '%(group)s': non vide."

#: ../usr/lib/usermanager/user.py:260 ../usr/lib/usermanager/user.py:332
#, python-format
msgid "The command '%(cmd)s' returned error code: %(ret)d."
msgstr "La commande '%(cmd)s' renvoie un code d'erreur: %(ret)d."

#: ../usr/lib/usermanager/user.py:330
#, python-format
msgid ""
"Cannot remove user '%(user)s': processes started by user are still running."
msgstr ""
"Impossible de supprimer l'utilisateur '%(user)s': des processus sont en "
"cours d'exécution."

#: ../usr/share/kde4/services/kcm_usermanager.desktop.in.h:1
#: ../usr/share/applications/usermanager.desktop.in.h:2
msgid "User management"
msgstr "Gestion des utilisateurs"

#: ../usr/share/kde4/services/kcm_usermanager.desktop.in.h:2
#: ../usr/share/applications/usermanager.desktop.in.h:1
msgid "Manage users"
msgstr "Gestion des droits des utilisateurs"
