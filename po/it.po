#: ../usr/lib/usermanager/usermanager.py:101
msgid "Disabled"
msgstr "DIsattivato"

#: ../usr/lib/usermanager/usermanager.py:104
msgid "Privileges"
msgstr "Permessi"

#: ../usr/lib/usermanager/usermanager.py:106
msgid "Password"
msgstr "Password"

#: ../usr/lib/usermanager/usermanager.py:108
msgid "Last changed"
msgstr "Ultima modifica"

#: ../usr/lib/usermanager/usermanager.py:109
msgid "Valid until"
msgstr "Attivo fino a"

#: ../usr/lib/usermanager/usermanager.py:110
