#: ../usr/lib/usermanager/usermanager.py:87
msgid "User manager"
msgstr "Gestore utenti"

#: ../usr/lib/usermanager/usermanager.py:88
msgid "Users"
msgstr "Utenti"

#: ../usr/lib/usermanager/usermanager.py:89
msgid "Groups"
msgstr "Gruppi"

#: ../usr/lib/usermanager/usermanager.py:91
msgid "User groups"
msgstr "Gruppi dell'utente"

#: ../usr/lib/usermanager/usermanager.py:93
msgid "Group accounts"
msgstr "Account gruppi"

#: ../usr/lib/usermanager/usermanager.py:95
