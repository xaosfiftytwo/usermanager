msgstr "Impostazioni utente"

#: ../usr/lib/usermanager/usermanager.py:96
msgid "Details"
msgstr "Dettagli"

#: ../usr/lib/usermanager/usermanager.py:97
msgid "Status"
msgstr "Stato"

