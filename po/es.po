msgid "You cannot remove the currently logged in user's primary group"
msgstr "Emposible eliminar el grupo primario del usuario actual"

#: ../usr/lib/usermanager/usermanager.py:590
msgid "Remove account"
msgstr "Eliminar cuenta"

#: ../usr/lib/usermanager/usermanager.py:590
msgid "You cannot remove an account from its own primary group"
msgstr "Imposible eliminar una cuenta de su propio grupo primario"

#: ../usr/lib/usermanager/user.py:247
#, python-format
msgid "Cannot remove group '%(group)s': not empty."
msgstr "Imposible eliminar el grupo '%(group)s': no está vacío."

#: ../usr/lib/usermanager/user.py:249 ../usr/lib/usermanager/user.py:321
#, python-format
msgid "The command '%(cmd)s' returned error code: %(ret)d."
msgstr "El comando '%(cmd)s' devolvió el código de error: %(ret)d."

#: ../usr/lib/usermanager/user.py:319
#, python-format
msgid ""
"Cannot remove user '%(user)s': processes started by user are still running."
msgstr "Imposible eliminar el usuario '%(user)s': tiene procesos iniciados."

