msgid "Save user settings"
msgstr "Guardar preferencias de usuario"

#: ../usr/lib/usermanager/usermanager.py:424
#, python-format
msgid "User saved: %(user)s"
msgstr "Usuario guardado: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:434
#, python-format
msgid "Nothing was changed on user: %(user)s"
msgstr "No se cambión nada para el usuario: %(user)s"

#: ../usr/lib/usermanager/usermanager.py:489
msgid "Select user image"
msgstr "Seleccionar imagen de usuario"

#: ../usr/lib/usermanager/usermanager.py:559
msgid "Remove group"
msgstr "Eliminar grupo"

#: ../usr/lib/usermanager/usermanager.py:561
#, python-format
msgid ""
"Are you sure you want to remove the following group:\n"
"\n"
"'%(group)s'"
msgstr ""
"Confirme que desea eliminar el siguiente grupo:\n"
"\n"
"'%(group)s'"

#: ../usr/lib/usermanager/usermanager.py:566
#, python-format
msgid "Group successfully removed: %(group)s"
msgstr "Grupo eliminado con éxito: %(group)s"

#: ../usr/lib/usermanager/usermanager.py:570
#, python-format
msgid "Could not remove group: %(group)s %(retmsg)s"
msgstr "Imposible eliminar el grupo: %(group)s %(retmsg)s"

